extends ../layout

block content
   .container
      .row
        .col-md-7
           embed(src="#{movie.flash}", allowFullScreen="true",quality="high", width="720",height="600",
                  align="middle",type="application/x-shockwave-flash")
        .col-md-5
           dl.dl-horizontal
               dt URL地址
               dd=movie.title
               dt 用户名
               dd=movie.doctor
               dt 密码
               dd=movie.country
               dt 语言
               dd=movie.language
               dt 上映年份
               dd=movie.year
               dt 简介
               dd=movie.summary




